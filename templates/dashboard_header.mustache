{{!
    @template liftoff/dashboard_header

    This template renders the header.

    Example context (json):
    {
        "pageheadingbutton": "page_heading_button_html"
    }

    Page header.
}}
<div class="container-fluid">
    <header class="row justify-content-between lec-dashboard-header">
        <div class="col-4">
            <h2>Dashboard</h2>
        </div>
        <div id="course-header" class="col-8">
            <div class="pull-right">
                {{{pageheadingbutton}}}
            </div>
        </div>
    </header>
</div>